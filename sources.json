    "mode": "api",
    "api_endpoint": "https://api.faufcc.ru/api/publications?type=news&page=1&limit=50",
    "include_patterns": [
      "/press-tsentr/novosti",
      "/press-center/news"
    ],
    "link_min_text_len": 0,
    "accept_empty_anchor": true,
    "include_regex": "/press-(?:tsentr/novosti|center/news)/[^/?#]+/?$",
    "restrict_domain": true,
    "max_links": 50,
    "enabled": true
  },
  {
    "name": "АНЦБ",
    "base_url": "http://ancb.ru",
