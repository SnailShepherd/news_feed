  {
    "name": "Парламентская газета: Экономика",
    "base_url": "https://www.pnp.ru",
    "start_url": "https://www.pnp.ru/economics/",
    "include_patterns": [
      "/economics/"
    ],
    "link_min_text_len": 0,
    "accept_empty_anchor": true,
    "include_regex": "^https?://(?:www\\.)?pnp\\.ru/economics/(?:\\d{4}/\\d{2}/\\d{2}/)?[^/]+\\.html$",
    "exclude_regex": "/economics/\\d+/?$",
    "restrict_domain": true,
    "enabled": true,
    "request_strategy": {
      "connect_timeout": 6,
      "read_timeout": 30,
      "max_attempts": 4,
      "backoff_factor": 1.5,
      "retry_statuses": [
        403
      ],
      "extra_headers": {
        "Accept-Language": "ru-RU,ru;q=0.9,en;q=0.8",
        "Referer": "https://www.pnp.ru/",
        "Sec-Fetch-Dest": "document",
        "Sec-Fetch-Mode": "navigate",
        "Sec-Fetch-Site": "same-origin",
        "Sec-Fetch-User": "?1"
      },
      "warmup": {
        "url": "https://www.pnp.ru/economics/",
        "delay": [
          2,
          4
        ],
        "timeout": 40
      },
      "selenium_fallback": true,
      "selenium_wait": 5,
      "capture_cookies": true,
      "record_path_on_success": true
    }
  },
